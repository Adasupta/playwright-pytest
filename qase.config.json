{
  "mode": "testops",
  "fallback": "report",
  "debug": true,
  "testops": {
    "project": "<add project code here>",
    "api": {
      "token": "<add token here>",
      "host": "qase.io"
    },
    "run": {
      "title": "pytestrun"
    },
    "batch": {
      "size": 100
    }
  },
  "report": {
    "driver": "local",
    "connection": {
      "local": {
        "path": "./build/qase-report",
        "format": "json"
      }
    }
  },
  "framework": {
    "pytest": {
      "captureLogs": true
    }
  },
  "userV2": true,
}
  
